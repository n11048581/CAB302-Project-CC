<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>




<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="app.LoggedInController"
            prefHeight="400.0" prefWidth="600.0">


    <VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1">
        <children>
            <AnchorPane prefHeight="48.0" prefWidth="600.0" style="-fx-background-color: grey;">
                <children>
                    <Label layoutX="281.0" layoutY="15.0" text="Header" />
                </children>
            </AnchorPane>
            <AnchorPane prefHeight="353.0" prefWidth="600.0" style="-fx-background-color: #ede7e5;">
                <children>
                    <TextField fx:id="tf_username" layoutX="226.0" layoutY="120.0" promptText="Username" />
                    <TextField fx:id="tf_password" layoutX="226.0" layoutY="164.0" promptText="Password" />
                    <Label layoutX="221.0" layoutY="58.0" text="Welcome!">
                        <font>
                            <Font name="Arial Rounded MT Bold" size="33.0" />
                        </font>
                    </Label>
                    <Button fx:id="button_login" layoutX="264.0" layoutY="234.0" mnemonicParsing="false" text="Login">
                        <font>
                            <Font size="19.0" />
                        </font>
                    </Button>
                    <Label layoutX="231.0" layoutY="199.0" text="Not a user?">
                        <font>
                            <Font name="Arial Rounded MT Bold" size="12.0" />
                        </font>
                    </Label>
                    <Button fx:id="button_signup" layoutX="298.0" layoutY="195.0" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Sign Up!" textFill="#0400ff" underline="true">
                        <font>
                            <Font name="Arial Rounded MT Bold" size="12.0" />
                        </font>
                    </Button>
                </children>
            </AnchorPane>
        </children>
    </VBox>


</AnchorPane>
